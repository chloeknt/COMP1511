// Scans characters from standard input, and checks whether all of the 
// brackets in the input are correctly matched.



#include <stdio.h>
#include <stdlib.h>

#define MAX 100

int main(void) {
    int brackets[MAX] = {0};
    int ch = getchar();
    int i = 0;
    while (ch != EOF) {
        if () {
            brackets[i] = ch;
            i++;
        }
        ch = getchar();
    }
    if () {
    printf("Invalid Sequence, the correct closing sequence is:\n");
    } else {
    printf("This is fine!\n");
    }
}
